{
  "short_name": "React App",
  "name": "React Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "version": "1.0",
  "permissions": ["https://*/", "http://*/", "activeTab", "tabs", "<all_urls>"],
  "content_security_policy": "script-src https://* 'self' https://platform.twitter.com https://cdn.syndication.twimg.com; object-src 'self'",
  "manifest_version": 2
}

